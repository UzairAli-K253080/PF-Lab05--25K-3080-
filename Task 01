#include <stdio.h>

int main() {
    char color;
    printf("Enter traffic light color (R for Red, Y for Yellow, G for Green): ");
    scanf(" %c", &color);

    if (color == 'R' || color == 'r') {
        printf("Stop\n");
    } else {
        if (color == 'Y' || color == 'y') {
            printf("Caution\n");
        } else {
            if (color == 'G' || color == 'g') {
                printf("Go\n");
            } else {
                printf("Invalid color\n");
            }
        }
    }

    return 0;
}
